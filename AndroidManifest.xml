<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="website.copyandpaste.bottombarnavigationwithnavigationdrawer">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Kechub1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity">
        </activity>
        <activity android:name=".Contact"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".Help">
        </activity>
        <activity android:name=".About">
        </activity>
        <activity android:name=".Admission">
        </activity>
        <activity android:name=".Training">

        </activity>
        <activity android:name=".Department"></activity>
        <activity android:name=".MainActivity2"></activity>
        <activity android:name=".Career"></activity>
        <activity android:name=".campus">
        </activity>
    </application>

</manifest>